#: wtforms/validators.py:58
#, python-format
msgid "Invalid field name '%s'."
msgstr "Nieprawidłowa nazwa pola '%s'."

#: wtforms/validators.py:66
#, python-format
msgid "Field must be equal to %(other_name)s."
msgstr "Wartość pola musi być równa %(other_name)s."

#: wtforms/validators.py:99
#, python-format
msgid "Field must be at least %(min)d character long."
msgid_plural "Field must be at least %(min)d characters long."
msgstr[0] "Pole musi mieć przynajmniej %(min)d znak."
msgstr[1] "Pole musi mieć przynajmniej %(min)d znaki."
msgstr[2] "Pole musi mieć przynajmniej %(min)d znaków."

#: wtforms/validators.py:102
#, python-format
msgid "Field cannot be longer than %(max)d character."
msgid_plural "Field cannot be longer than %(max)d characters."
msgstr[0] "Wartość w polu nie może mieć więcej niż %(max)d znak."
msgstr[1] "Wartość w polu nie może mieć więcej niż %(max)d znaki."
msgstr[2] "Wartość w polu nie może mieć więcej niż %(max)d znaków."

#: wtforms/validators.py:105
#, python-format
msgid "Field must be between %(min)d and %(max)d characters long."
msgstr "Wartość musi być długa na od %(min)d do %(max)d znaków."

#: wtforms/validators.py:141
#, python-format
msgid "Number must be at least %(min)s."
msgstr "Liczba musi być większa lub równa %(min)s."

#: wtforms/validators.py:143
#, python-format
msgid "Number must be at most %(max)s."
msgstr "Liczba musi być mniejsza lub równa %(max)s."

#: wtforms/validators.py:145
#, python-format
msgid "Number must be between %(min)s and %(max)s."
msgstr "Liczba musi być z zakresu %(min)s i %(max)s."

#: wtforms/validators.py:205 wtforms/validators.py:245
msgid "This field is required."
msgstr "To pole jest wymagane."

#: wtforms/validators.py:277
msgid "Invalid input."
msgstr "Nieprawidłowa wartość."

#: wtforms/validators.py:311
msgid "Invalid email address."
msgstr "Nieprawidłowy adres e-mail."

#: wtforms/validators.py:352
msgid "Invalid IP address."
msgstr "Nieprawidłowy adres IP."

#: wtforms/validators.py:403
msgid "Invalid Mac address."
msgstr "Nieprawidłowy adres Mac."

#: wtforms/validators.py:432
msgid "Invalid URL."
msgstr "Nieprawidłowy URL."

#: wtforms/validators.py:452
msgid "Invalid UUID."
msgstr "Nieprawidłowy UUID."

#: wtforms/validators.py:482
#, python-format
msgid "Invalid value, must be one of: %(values)s."
msgstr "Wartość musi być jedną z: %(values)s."

#: wtforms/validators.py:514
#, python-format
msgid "Invalid value, can't be any of: %(values)s."
msgstr "Wartość nie może być żadną z: %(values)s."

#: wtforms/csrf/core.py:98 wtforms/ext/csrf/form.py:47
msgid "Invalid CSRF Token"
msgstr "Nieprawidłowy token CSRF"

#: wtforms/csrf/session.py:61 wtforms/ext/csrf/session.py:58
msgid "CSRF token missing"
msgstr "Brak tokena CSRF"

#: wtforms/csrf/session.py:69 wtforms/ext/csrf/session.py:66
msgid "CSRF failed"
msgstr "błąd CSRF"

#: wtforms/csrf/session.py:74 wtforms/ext/csrf/session.py:71
msgid "CSRF token expired"
msgstr "Wygasł token CSRF"

